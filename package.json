{
  "name": "one-front",
  "version": "1.0.0",
  "description": "Universal React application for Renault and Dacia",
  "main": "build/server/index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "clean": "run-s clean:build",
    "clean:build": "rimraf build .happypack styleguide",
    "prerestore": "rm -rf ./node_modules && yarn install",
    "restore": "run-p clean copy-conf",
    "restore:min": "run-p clean copy-conf install",
    "copy-conf": "ncp config/local.json.example config/local.json",
    "install": "npm run flow:defs",
    "dev": "node tools/development",
    "dev:proxy": "run-p api:proxy dev",
    "dev:mocks": "run-p api:mocks dev",
    "api:proxy": "nodemon --watch config/local.json  tools/scripts/serveDevApiProxy",
    "api:mocks": "nodemon --watch mocks --watch config/local.json tools/scripts/serveDevApiMocks",
    "build": "node tools/scripts/build",
    "analyze": "node tools/scripts/analyze",
    "start": "node build/server",
    "styleguide": "styleguidist server",
    "styleguide:build-full": "node tools/scripts/styleguide renault full",
    "styleguide:build": "node tools/scripts/styleguide renault",
    "test": "run-s lint jest",
    "test:coverage": "run-s jest",
    "deploy": "node tools/scripts/deploy",
    "lint": "run-p lint:*",
    "lint:scripts": "eslint src acceptance/src",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "flow": "flow",
    "flow:report": "flow-coverage-report -i 'src/**/*.js' -x 'src/**/*.spec.js' -t html -t json -t text",
    "flow:remove": "run-s flow:remove:*",
    "flow:remove:types": "node tools/scripts/removeTypes.js",
    "flow:remove:script": "rimraf tools/scripts/removeTypes.js",
    "precommit": "run-s lint flow",
    "commitmsg": "validate-commit-msg",
    "prepush": "run-p jest",
    "jest": "BABEL_ENV=test jest --config ./tools/config/test/jest.cover.json",
    "jest:update": "BABEL_ENV=test jest -u --config ./tools/config/test/jest.straight.json",
    "jest:watch": "BABEL_ENV=test jest --watch --config ./tools/config/test/jest.straight.json",
    "doc:html": "documentation build --github -f html -o doc-html/ src/**",
    "doc:watch": "documentation serve --watch --github src/**",
    "mocks:import": "node tools/scripts/importMocks",
    "acceptance:tradein": "wdio acceptance/conf/wdio.local.conf.js -site tradein",
    "acceptance:uci": "wdio acceptance/conf/wdio.local.conf.js -site uci",
    "acceptance-exp:tradein": "wdio acceptance/conf/wdio.experimental.conf.js -site tradein",
    "acceptance-exp:uci": "wdio acceptance/conf/wdio.experimental.conf.js -site uci",
    "metadata:phones": "libphonenumber-generate-metadata src/shared/metadata/phones.min.json --countries FR --extended"
  },
  "keywords": [
    "wired",
    "helios",
    "renault",
    "dacia",
    "react",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "dependencies": {
    "app-root-dir": "^1.0.2",
    "autoprefixer": "^7.1.1",
    "axios": "^0.16.2",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.4.0",
    "babel-template": "^6.24.1",
    "body-parser": "^1.17.0",
    "browserstack-local": "^1.3.0",
    "chai": "^4.0.2",
    "classnames": "^2.2.5",
    "color": "^2.0.0",
    "compression": "1.6.2",
    "config": "^1.24.0",
    "cucumber": "^2.3.1",
    "dom-delegate": "^2.0.3",
    "dyson": "^1.2.2",
    "enzyme": "^2.9.1",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "express": "^4.15.4",
    "fbjs": "^0.8.12",
    "flow-runtime": "^0.14.0",
    "fontgen-loader": "^0.2.1",
    "glob": "^7.1.2",
    "google-map-react": "^0.24.0",
    "gridle": "^2.0.48",
    "helmet": "^3.6.1",
    "hpp": "^0.2.2",
    "http-proxy": "^1.16.2",
    "http-proxy-middleware": "^0.17.3",
    "http-proxy-rules": "^1.1.0",
    "jest-cli": "^20.0.4",
    "jsonp": "^0.2.1",
    "libphonenumber-js": "^0.4.16",
    "lodash": "^4.17.4",
    "match-require": "^2.1.0",
    "moment": "^2.17.1",
    "node-aigis": "^1.2.1",
    "normalize-scss": "^7.0.0",
    "object-path": "^0.11.4",
    "picturefill": "^3.0.2",
    "query-string": "^4.3.4",
    "rc-pagination": "^1.10.4",
    "rc-slider": "^8.2.0",
    "rc-tooltip": "^3.4.3",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-async-bootstrapper": "^1.1.1",
    "react-async-component": "^1.0.0-beta.3",
    "react-click-outside": "^2.3.0",
    "react-cookie": "^2.0.7",
    "react-dom": "^15.6.1",
    "react-flow-types": "^0.1.1",
    "react-helmet": "^5.0.3",
    "react-markdown": "^2.5.0",
    "react-maskedinput": "^3.3.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-scrollchor": "^3.0.0",
    "react-select": "^1.0.0-rc.3",
    "react-sidebar": "^2.3.2",
    "react-slick": "^0.14.11",
    "react-sticky": "^6.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.1.0",
    "sass-resources-loader": "^1.2.1",
    "serialize-javascript": "1.3.0",
    "source-map-support": "^0.4.14",
    "tcomb-form": "^0.9.17",
    "tcomb-json-schema": "^0.3.0",
    "uuid": "^3.1.0",
    "validate-commit-msg": "^2.12.2",
    "wdio-cucumber-framework": "^0.3.1",
    "wdio-selenium-standalone-service": "^0.0.9",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.7.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-runtime": "^0.11.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chokidar": "^1.7.0",
    "colors": "1.1.2",
    "css-loader": "^0.28.0",
    "documentation": "^4.0.0-rc.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.52.0",
    "flow-coverage-report": "^0.3.0",
    "flow-remove-types": "^1.2.0",
    "flow-typed": "2.1.5",
    "happypack": "^4.0.0-beta.2",
    "husky": "^0.14.3",
    "json-loader": "0.5.4",
    "md5": "2.2.1",
    "ncp": "^2.0.0",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "3.0.0-beta.6",
    "react-styleguidist": "^6.0.24",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.10.0",
    "rimraf": "2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.17.0",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-selector-bem-pattern": "^1.1.0",
    "url-loader": "0.5.8",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "^1.6.0"
  }
}
